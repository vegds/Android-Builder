[
  {
    "kernelSource": {
      "name": "vegds-MI10",
      "repo": "https://github.com/vegds/InfiniR_kernel_ucmi_NetHunter",
      "branch": "14.0-umi",
      "device": "umi",
      "defconfig": "ucmi_nethunter_defconfig"
    },
    "toolchains": [
      {
        "url": "https://github.com/ZyCromerZ/Clang/releases/download/17.0.0-20230725-release/Clang-17.0.0-20230725.tar.gz",
        "name": "clang",
        "binaryEnv": [
          "./bin"
        ]
      }
    ],
    "enableCcache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang",
      "externalCommands": {
       "CROSS_COMPILE": "aarch64-linux-gnu-",
          "CLANG_TRIPLE": "aarch64-linux-gnu-",
          "NM":"llvm-nm",
          "OBJDUMP":"llvm-objdump",
          "STRIP":"llvm-strip"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true
    },
    "enableKernelSU": true,
    "enableLXC": true
  }
]
